# tmux_bash_completion - Alan Christopher Thomas
# http://alanchristopherthomas.com/

_ta() {
    local word opts
    word="${COMP_WORDS[COMP_CWORD])}"
    opts="`tmux list-sessions | awk -F':' '{ORS=\" \"; print $1}'`"
    COMPREPLY=($(compgen -W "${opts}" -- ${word}))
    return 0
}
complete -F _ta ta
